#include <stdio.h>
void main() 
{
    int n;
    printf("Enter number of elements in the array: ");
    scanf("%d", &n);
 
    int a[n];
 
    
    printf("Enter %d elements in the array: ",n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
 
    
    printf("Even numbers in the array are: ");
    for(int i=0;i<n;i++)
    {
        if(a[i]%2==0)
            printf("%d ", a[i]);
    }
 
    
    printf("Odd numbers in the array are: ");
    for(int i=0;i<n;i++)
    {
        if(a[i]%2==1)
            printf("%d ", a[i]);
    }
}
